{% extends 'base.html.twig' %}

{% block title %}FDS{% endblock %}

{% block body %}
    <h1>FDS</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fds.id }}</td>
            </tr>
            <tr>
                <th>FilePath</th>
                <td>{{ fds.filePath }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ fds.createdAt ? fds.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ fds.updatedAt ? fds.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Version</th>
                <td>{{ fds.version }}</td>
            </tr>
            <tr>
                <th>ChemicalName</th>
                <td>{{ fds.chemicalName }}</td>
            </tr>
            <tr>
                <th>Practice</th>
                <td>{{ fds.practice }}</td>
            </tr>
            <tr>
                <th>DangerWarnings</th>
                <td>{{ fds.dangerWarnings }}</td>
            </tr>
            <tr>
                <th>CautionaryAdvice</th>
                <td>{{ fds.cautionaryAdvice }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_fds_index') }}">back to list</a>

    <a href="{{ path('app_fds_edit', {'id': fds.id}) }}">edit</a>

    {{ include('fds/_delete_form.html.twig') }}
{% endblock %}
